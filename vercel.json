{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "env": {
      "VITE_ESPRESSO_API_URL": "https://api.espresso.com"
    },
    "build": {
      "env": {
        "VITE_ESPRESSO_API_URL": "https://api.espresso.com"
      }
    },
   
    
    "rewrites": [
      {
        "source": "/embed",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/embed",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          }
        ]
      }
    ]
  }